#grazil_bribe
~
$_0 = str_to_int $_0
if !hasflag $n fortz_juz_chce
  tell $n Narazie nie z³o¿y³e¶ ¿adnego zamówienia.
  mob echoat $n Grazil mamrocze przez chwile, jakby siê zastanawiaj±c nad czym¶. Po chwili jednak z smutnym wyrazem twarzy oddaje ci pieni±dze.
  mob reward $n silver $_0
  break
else
  if hasflag $n fort_chce_miecz
  and numeval $_0 >= 80
    goto przedmiot_1
  endif
  if hasflag $n fort_chce_mlot
  and numeval $_0 >= 350
    goto przedmiot_2
  endif
  if hasflag $n fort_chce_topor
  and numeval $_0 >= 150
    goto przedmiot_3
  endif
  tell $n Nie przypominam sobie, ¿ebym da³ ci jaki¶ upust. Jak nie masz wiêcej srebra to id¼, okradnij kogo¶ i dopiero wróæ.
  mob echoat $n Grazil wzdycha poczym oddaje ci twoje pieni±dze.
  mob reward $n silver $_0
  break
endif
:przedmiot_1
tell $n Dobra, za pare dni mo¿esz zwróciæ siê po swój krótki ¿elazny miecz.
po odbiór.
mob setflag $n fort_zaplacil_miecz -1
mob setflag $n fortz_musi_odczekac 18
$_0 = int_sub $_0 80
if numeval $_0 > 0
  tell $n Szlag, ¿eby nie moja uczciwa dusza nie odzyska³by¶ tych pieniêdzy.
  mob echoat $n Grazil odlicza czê¶æ pieniêdzy i oddaje ci resztê.
  mob reward $n silver $_0
endif
break
:przedmiot_2
tell $n Zrobiæ dwurêczny m³ot to nie chop siup. Zg³o¶ siê po niego za parê dni.
mob setflag $n fort_zaplacil_mlot -1
mob setflag $n fortz_musi_odczekac 30
$_0 = int_sub $_0 350
if numeval $_0 > 0
  Emote przelicza pieni±dze po czym z wyra¼n± z³o¶ci± zwraca siê do ciebie.
  minidelay 1
  tell $n Cholera, naucz siê liczyæ pieni±dze i dawaæ odliczone kwoty.
  mob echoat $n Grazil oddaje ci resztê.
  mob reward $n silver $_0
endif
break
:przedmiot_3
tell $n Nad tym ¿elaznym toporem bêdê siê musia³ pomêczyæ. Za jaki¶ czas bêdzie gotowy.
mob setflag $n fort_zaplacil_topor -1
mob setflag $n fortz_musi_odczekac 24
$_0 = int_sub $_0 150
if numeval $_0 >= 1
  tell $n Masz tu reszte, $_0 monet.
  mob echoat $n Grazil odlicza odpowiedni± sumê i oddaje ci j±.
  mob reward $n silver $_0
endif
break
break
end
